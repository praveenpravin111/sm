#include<iostream>
using namespace std;
int main()
{
signed long num1=0;
cout<<"\nto find whether entered number is positive or not";
cout<<"\ngive an input number";
cin>>num1;
if(num1>0)
{
cout<<"\nentered num is positive";
}
else if(num1<0)
{
cout<<"\nentered num is negative";
}
else
{
cout<<"\n number is zero";
}
return 0;
}
